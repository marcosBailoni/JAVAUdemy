package fundamentos;

public class Temperatura {

	public static void main(String[] args) {
		
		final double A = 32;
		
		final double B = 5.0/9;
		
		float F = 86;

		double C = (F - A) * B;
		
		System.out.println("Temperatura convertida: " + C + "CÂº");
				
		
		
		//(F - A) * B = C
		// (F - 32) x 5/9 = C
	}
}
