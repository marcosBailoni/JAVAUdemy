package streams;

import java.util.Arrays;
import java.util.List;
import java.util.function.Function;
import java.util.function.UnaryOperator;

public class DesafioMap {

	public static void main(String[] args) {
		
		 List<Integer> nums = Arrays.asList(1, 2, 3, 4, 5, 6, 7, 8, 9);
		 
		 Function<Integer, String> getHex = n -> Integer.toHexString(n);
		 
		 System.out.println(getHex.apply(5) + 2);
		 
		 System.out.println(Integer.toHexString(5));
		 
		 int num = 2;
		 String hex = Integer.toHexString(num);
		 
		 System.out.println(hex + " teste");
//		 nums.stream().map(getHex).forEach(System.out::print);
	
	}
}
