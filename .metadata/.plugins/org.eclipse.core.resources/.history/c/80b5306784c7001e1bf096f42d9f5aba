package excecao;

public class Basico {

	public static void main(String[] args) {
		
		Aluno a1 = null;
		
		// Abaixo, ir치 retornar o erro para a vari치vel "e" e depois imprimir o erro;
		try {
			System.out.println(a1.nome);
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		//Abaixo ir치 retornar o print no lugar do erro, guardando a exercao na vari치vel "e";
		try {
			System.out.println(a1.nome);
		} catch (Exception e) {
			System.out.println("Erro para imprimir o nome do Aluno!!!");;
		}
		
		System.out.println(7 / 0);

		System.out.println("Fim");
	}
}
