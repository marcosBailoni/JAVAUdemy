package lambdas;

public class Produto {

	String nome;
	double preco;
	double desconto;
	
	public Produto(String nome, double preco, double desconto) {			
		this.nome = nome;
		this.preco = preco;
		this.desconto = desconto;
	}
	
	 public String toString() {
		 return "Nome: " + nome + "Tem pre√ßo de R$" + preco + "e desconto de" + desconto;
				  			 
	 }
	

}
