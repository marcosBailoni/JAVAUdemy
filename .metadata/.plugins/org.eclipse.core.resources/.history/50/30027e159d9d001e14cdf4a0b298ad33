package arrays;

import java.util.Arrays;

public class Exercicios {
	
	public static void main(String[] args) {
		
		double[] notasAlunoA = new double[3];
	
		notasAlunoA[0] = 7.9;
		notasAlunoA[1] = 8;
		notasAlunoA[0] = 6.7;
	
		String notasAlunoAEmString = Arrays.toString(notasAlunoA);
		System.out.println(notasAlunoAEmString);
		
		System.out.println(notasAlunoA);
		
	
	}

}
