package fundamentos;

public class OperadoresLogicos {
	
	public static void main(String[] args) {
		
		boolean cond1 = true;
		boolean cond2 = 3 > 7;
		
		System.out.println(cond1 && !cond2);
		System.out.println(cond1 || cond2);
		System.out.println(cond1 ^ cond2);
		System.out.println(!!cond1);
		System.out.println(!cond2);
		
	}
}
